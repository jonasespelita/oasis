<div class="grid_8" id = "reg">
  <div id="sos"></div>
  <div id="top_r"></div>
  <div id="side_r">
    <b>To register, enter your ward&#39;s ID number and received verification code.</b>
    <br/><br/>

    <%= error_messages_for :user %>
<% form_for :user, :url => users_path do |f| -%>
  <p><label id="fst"for="login">Login</label><br/>
  <%= f.text_field :login %></p>

  <p><label id="fst" for="email">Email</label><br/>
  <%= f.text_field :email %></p>

  <p><label id="fst"for="password">Password</label><br/>
  <%= f.password_field :password %></p>

  <p><label id="fst"for="password_confirmation">Confirm Password</label><br/>
  <%= f.password_field :password_confirmation %></p>

  <p><%= submit_tag 'Sign up' %></p>
<% end -%>


